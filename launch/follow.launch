<launch>
  <!-- Launch the face detector -->
	<include file="$(find rail_face_detector_launch)/launch/face_detector.launch">
    <arg name="wg_detector" value="true" />
  </include>

  <!-- Launch the laser filter and the leg detector -->
	<include file="$(find map_laser)/lfilter.launch"/>
	<include file="$(find leg_tracker)/launch/joint_leg_tracker.launch"/>

  <!-- Launch the node to follow people -->
	<node name="follow" pkg="fetch_demos" type="follow.py" output="screen" />
</launch>
